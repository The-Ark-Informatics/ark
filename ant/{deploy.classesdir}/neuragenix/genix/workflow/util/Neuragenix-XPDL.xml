<?xml version="1.0"?>

<Package Id="neuragenix.genix.workflow.fillPatientDetails" Name="FillPatientDetails">
    <PackageHeader>
        <XPDLVersion>1.0</XPDLVersion>
        <Vendor>Neuragenix</Vendor>
		<Created>02/02/2004</Created>
        <Description>Fill Patient Details Workflow</Description>
    </PackageHeader>

    <Participants>
        <Participant Id="User1" Name="Workflow Admin">
            <ParticipantType Type="HUMAN"/>
            <Description>None</Description>
        </Participant>

        <Participant Id="User2" Name="Agustian Agustian">
        	<ParticipantType Type="HUMAN"/>
        	<Description>Normal User</Description>
        </Participant>
    </Participants>

    <WorkflowProcesses>
        <WorkflowProcess Id="FillPatientDetails" Name="Fill Patient Details" InitialActivityId="addPatientConsent">
            <ProcessHeader>
				<Created>02/02/2004</Created>
				<Description>Fill Patient Details</Description>
            </ProcessHeader>

			<Parameters>
				<Parameter Name="Parameter1" Type="Integer" Desc="Description 1" />
				<Parameter Name="Parameter2" Type="Integer" Desc="Description 2" />
				<Parameter Name="Parameter3" Type="Integer" Desc="Description 3" />
			</Parameters>

			<Triggers>
				<Trigger Id="Trigger1" Name="Trigger 1" Action="add_patient">
					<Domain>patient</Domain>
					<Field>dtDOB</Field>
					<Type>Date</Type>
					<Connection1></Connection1>
					<Operator1>=</Operator1>
					<Value1>01/01/2000</Value1>
					<Connection2></Connection2>
					<Operator2></Operator2>
					<Value2></Value2>
				</Trigger>
				<Trigger Id="Trigger2" Name="Trigger 2" Action="add_patient">
					<Domain>consent</Domain>
					<Field>dtConsentedDate</Field>
					<Type>Date</Type>
					<Connection1>AND</Connection1>
					<Operator1>=</Operator1>
					<Value1>01/01/2004</Value1>
					<Connection2>OR</Connection2>
					<Operator2>=</Operator2>
					<Value2>10/10/2004</Value2>
				</Trigger>
			</Triggers>


			<Activities>
				<Activity Id="startPatientWorkflow" Name="Start Patient Workflow">
					<Description>start patient workflow</Description>
					<Performer></Performer>
					<PerformerType></PerformerType>
					<Priority></Priority>
					<Params>
					</Params>
					<ExtendedAttributes>
						<ExtendedAttribute Name="strType" Value="Start"/>
						<ExtendedAttribute Name="intPosX" Value="10"/>
						<ExtendedAttribute Name="intPosY" Value="20"/>
						<ExtendedAttribute Name="intWidth" Value="30"/>
						<ExtendedAttribute Name="intHeight" Value="40"/>
						<ExtendedAttribute Name="strBorderColor" Value="Black"/>
						<ExtendedAttribute Name="strBackgroundColor" Value="White"/>
                    </ExtendedAttributes>
				</Activity>

				<Activity Id="stopPatientWorkflow" Name="Stop Patient Workflow">
					<Description>stop patient workflow</Description>
                    <Performer></Performer>
					<PerformerType></PerformerType>
					<Priority></Priority>
					<Params>
					</Params>
					<ExtendedAttributes>
						<ExtendedAttribute Name="strType" Value="Stop"/>
						<ExtendedAttribute Name="intPosX" Value="10"/>
						<ExtendedAttribute Name="intPosY" Value="20"/>
						<ExtendedAttribute Name="intWidth" Value="30"/>
						<ExtendedAttribute Name="intHeight" Value="40"/>
						<ExtendedAttribute Name="strBorderColor" Value="Black"/>
						<ExtendedAttribute Name="strBackgroundColor" Value="White"/>
					</ExtendedAttributes>
				</Activity>
				
                <Activity Id="addPatient" Name="Add Patient">
					<Description>Initial activity when this workflow is initiated</Description>
                    <Performer>1</Performer>
					<PerformerType>type1</PerformerType>
					<Priority>Low</Priority> 
					<Params>
						<Param Name="Param1" Type="Date" Desc="Description 1" />
						<Param Name="Param2" Type="Date" Desc="Description 2"/>
					</Params>
                    <ExtendedAttributes>
                        <ExtendedAttribute Name="strReassignable" Value="y"/>
						<ExtendedAttribute Name="strSignoff" Value="n"/>
						<ExtendedAttribute Name="strInstruction" Value="add patient"/>
						<ExtendedAttribute Name="intFunctionKey" Value="1"/>
						<ExtendedAttribute Name="strIfFail" Value=""/>
						<ExtendedAttribute Name="strRetryDelayUnit" Value="Hour"/>
						<ExtendedAttribute Name="flRetryDelayValue" Value="10"/>
						<ExtendedAttribute Name="intRetryCounter" Value="1"/>
						<ExtendedAttribute Name="strIfFailAfterRetry" Value="Stop"/>
						<ExtendedAttribute Name="intCompletedTask" Value="1"/>
						<ExtendedAttribute Name="strMultiAction" Value="yes"/>
						<ExtendedAttribute Name="strTriggerUnit" Value="Month"/> 
						<ExtendedAttribute Name="flTriggerValue" Value="10.5"/>
						<ExtendedAttribute Name="strFinishedUnit" Value="Month"/>
						<ExtendedAttribute Name="flFinishedValue" Value="10.5"/>
						<ExtendedAttribute Name="strIntervalUnit" Value="Daily"/>
						<ExtendedAttribute Name="strIntervalValue" Value="6:00PM"/>
						<ExtendedAttribute Name="strType" Value="Human"/>
						<ExtendedAttribute Name="intPosX" Value="10"/>
						<ExtendedAttribute Name="intPosY" Value="20"/>
						<ExtendedAttribute Name="intWidth" Value="30"/>
						<ExtendedAttribute Name="intHeight" Value="40"/>
						<ExtendedAttribute Name="strBorderColor" Value="Black"/>
						<ExtendedAttribute Name="strBackgroundColor" Value="White"/>
                    </ExtendedAttributes>
                </Activity>
                
                <Activity Id="addPatientConsent" Name="Add Patient Consent">
					<Description>Second Activity in this workflow</Description>
                    <Performer>2</Performer>
					<PerformerType>type1</PerformerType>
					<Priority>Low</Priority>
					<Params>
						<Param Name="strNameChecked" Type="String" Desc="the checked name" />
						<Param Name="intData" Type="Integer" Desc="the data" />
					</Params>
					<ExtendedAttributes>
						<ExtendedAttribute Name="strReassignable" Value="y"/>
						<ExtendedAttribute Name="strSignoff" Value="n"/>
						<ExtendedAttribute Name="strInstruction" Value="add patient consent"/>
						<ExtendedAttribute Name="intFunctionKey" Value="1"/>
						<ExtendedAttribute Name="strIfFail" Value="Continue"/>
						<ExtendedAttribute Name="strRetryDelayUnit" Value="Second"/>
						<ExtendedAttribute Name="flRetryDelayValue" Value="10"/>
						<ExtendedAttribute Name="intRetryCounter" Value="3"/>
						<ExtendedAttribute Name="strIfFailAfterRetry" Value="Continue"/>
						<ExtendedAttribute Name="intCompletedTask" Value="1"/>
						<ExtendedAttribute Name="strMultiAction" Value="yes"/>
						<ExtendedAttribute Name="strTriggerUnit" Value="Month"/> 
						<ExtendedAttribute Name="flTriggerValue" Value="10.5"/> 
						<ExtendedAttribute Name="strFinishedUnit" Value="Month"/>
						<ExtendedAttribute Name="flFinishedValue" Value="10.5"/>
						<ExtendedAttribute Name="strIntervalUnit" Value="Daily"/>
						<ExtendedAttribute Name="strIntervalValue" Value="6:00PM"/>
						<ExtendedAttribute Name="strType" Value="System"/>
						<ExtendedAttribute Name="intPosX" Value="10"/>
						<ExtendedAttribute Name="intPosY" Value="20"/>
						<ExtendedAttribute Name="intWidth" Value="30"/>
						<ExtendedAttribute Name="intHeight" Value="40"/>
						<ExtendedAttribute Name="strBorderColor" Value="Black"/>
						<ExtendedAttribute Name="strBackgroundColor" Value="White"/>
                    </ExtendedAttributes>
				</Activity>

				<Activity Id="doctorWorkflow" Name="Doctor Workflow">
					<Description>Third Activity in this workflow</Description>
                    <Performer></Performer>
					<PerformerType></PerformerType>
					<Priority></Priority>
					<Params>
					</Params>
					<ExtendedAttributes>
						<ExtendedAttribute Name="strType" Value="Sub workflow"/>
						<ExtendedAttribute Name="strSubWorkflowPackageXPDLId" Value="neuragenix.genix.workflow.fillDoctorDetails"/>
						<ExtendedAttribute Name="strSubWorkflowTemplateXPDLId" Value="FillDoctorDetails"/>
						<ExtendedAttribute Name="intCompletedTask" Value="1"/>
						<ExtendedAttribute Name="intPosX" Value="10"/>
						<ExtendedAttribute Name="intPosY" Value="20"/>
						<ExtendedAttribute Name="intWidth" Value="30"/>
						<ExtendedAttribute Name="intHeight" Value="40"/>
						<ExtendedAttribute Name="strBorderColor" Value="Black"/>
						<ExtendedAttribute Name="strBackgroundColor" Value="White"/>
                    </ExtendedAttributes>
				</Activity>

				<Activity Id="nurseWorkflow" Name="Nurse Workflow">
					<Description>Forth Activity in this workflow</Description>
                    <Performer></Performer>
					<PerformerType></PerformerType>
					<Priority></Priority>
					<Params>
					</Params>
					<ExtendedAttributes>
						<ExtendedAttribute Name="strType" Value="Sub workflow"/>
						<ExtendedAttribute Name="strSubWorkflowPackageXPDLId" Value="neuragenix.genix.workflow.fillNurseDetails"/>
						<ExtendedAttribute Name="strSubWorkflowTemplateXPDLId" Value="FillNurseDetails"/>
						<ExtendedAttribute Name="intCompletedTask" Value="2"/>
						<ExtendedAttribute Name="intPosX" Value="10"/>
						<ExtendedAttribute Name="intPosY" Value="20"/>
						<ExtendedAttribute Name="intWidth" Value="30"/>
						<ExtendedAttribute Name="intHeight" Value="40"/>
						<ExtendedAttribute Name="strBorderColor" Value="Black"/>
						<ExtendedAttribute Name="strBackgroundColor" Value="White"/>
                    </ExtendedAttributes>
				</Activity>

                <Activity Id="addPatientAgain" Name="Add Patient Again">
					<Description>to add patient again</Description>
                    <Performer>1</Performer>
					<PerformerType>type1</PerformerType>
					<Priority>Low</Priority> 
					<Params>
						<Param Name="Param1" Type="Date" Desc="Description 1" />
						<Param Name="Param2" Type="Date" Desc="Description 2"/>
					</Params>
                    <ExtendedAttributes>
                        <ExtendedAttribute Name="strReassignable" Value="y"/>
						<ExtendedAttribute Name="strSignoff" Value="n"/>
						<ExtendedAttribute Name="strInstruction" Value="add patient"/>
						<ExtendedAttribute Name="intFunctionKey" Value="1"/>
						<ExtendedAttribute Name="strIfFail" Value=""/>
						<ExtendedAttribute Name="strRetryDelayUnit" Value="Hour"/>
						<ExtendedAttribute Name="flRetryDelayValue" Value="10"/>
						<ExtendedAttribute Name="intRetryCounter" Value="1"/>
						<ExtendedAttribute Name="strIfFailAfterRetry" Value="Stop"/>
						<ExtendedAttribute Name="intCompletedTask" Value="1"/>
						<ExtendedAttribute Name="strMultiAction" Value="yes"/>
						<ExtendedAttribute Name="strTriggerUnit" Value="Month"/> 
						<ExtendedAttribute Name="flTriggerValue" Value="10.5"/>
						<ExtendedAttribute Name="strFinishedUnit" Value="Month"/>
						<ExtendedAttribute Name="flFinishedValue" Value="10.5"/>
						<ExtendedAttribute Name="strIntervalUnit" Value="Daily"/>
						<ExtendedAttribute Name="strIntervalValue" Value="6:00PM"/>
						<ExtendedAttribute Name="strType" Value="Human"/>
						<ExtendedAttribute Name="intPosX" Value="10"/>
						<ExtendedAttribute Name="intPosY" Value="20"/>
						<ExtendedAttribute Name="intWidth" Value="30"/>
						<ExtendedAttribute Name="intHeight" Value="40"/>
						<ExtendedAttribute Name="strBorderColor" Value="Black"/>
						<ExtendedAttribute Name="strBackgroundColor" Value="White"/>
                    </ExtendedAttributes>
				</Activity>

				<Activity Id="addPatientAgain2" Name="Add Patient Again2">
					<Description>to add patient again 2</Description>
                    <Performer>1</Performer>
					<PerformerType>type1</PerformerType>
					<Priority>Low</Priority> 
					<Params>
						<Param Name="Param1" Type="Date" Desc="Description 1" />
						<Param Name="Param2" Type="Date" Desc="Description 2"/>
					</Params>
                    <ExtendedAttributes>
                        <ExtendedAttribute Name="strReassignable" Value="y"/>
						<ExtendedAttribute Name="strSignoff" Value="n"/>
						<ExtendedAttribute Name="strInstruction" Value="add patient"/>
						<ExtendedAttribute Name="intFunctionKey" Value="1"/>
						<ExtendedAttribute Name="strIfFail" Value=""/>
						<ExtendedAttribute Name="strRetryDelayUnit" Value="Hour"/>
						<ExtendedAttribute Name="flRetryDelayValue" Value="10"/>
						<ExtendedAttribute Name="intRetryCounter" Value="1"/>
						<ExtendedAttribute Name="strIfFailAfterRetry" Value="Stop"/>
						<ExtendedAttribute Name="intCompletedTask" Value="2"/>
						<ExtendedAttribute Name="strMultiAction" Value="yes"/>
						<ExtendedAttribute Name="strTriggerUnit" Value="Month"/> 
						<ExtendedAttribute Name="flTriggerValue" Value="10.5"/>
						<ExtendedAttribute Name="strFinishedUnit" Value="Month"/>
						<ExtendedAttribute Name="flFinishedValue" Value="10.5"/>
						<ExtendedAttribute Name="strIntervalUnit" Value="Daily"/>
						<ExtendedAttribute Name="strIntervalValue" Value="6:00PM"/>
						<ExtendedAttribute Name="strType" Value="Human"/>
						<ExtendedAttribute Name="intPosX" Value="10"/>
						<ExtendedAttribute Name="intPosY" Value="20"/>
						<ExtendedAttribute Name="intWidth" Value="30"/>
						<ExtendedAttribute Name="intHeight" Value="40"/>
						<ExtendedAttribute Name="strBorderColor" Value="Black"/>
						<ExtendedAttribute Name="strBackgroundColor" Value="White"/>
                    </ExtendedAttributes>
                </Activity>
			</Activities>

            <Transitions>
				<Transition Id="trans1.0" From="startPatientWorkflow" To="addPatientConsent" Name="Goto Add Patient Consent" Condition="" Type="Normal" XCoord="1 2 3" YCoord="1 2 3">
				</Transition>
				<Transition Id="trans1.1" From="addPatientConsent" To="addPatient" Name="Goto Add Patient" Condition="if(intData > 10){return Boolean.valueOf(true);}else{return Boolean.valueOf(false);}" Type="Normal" XCoord="1 2 3" YCoord="1 2 3">
                </Transition>
				<Transition Id="trans1.2" From="addPatientConsent" To="doctorWorkflow" Name="Goto Doctor Workflow" Condition="" Type="Normal" XCoord="1 2 3" YCoord="1 2 3">
				</Transition>
				<Transition Id="trans1.3" From="addPatient" To="nurseWorkflow" Name="Goto Nurse Workflow" Condition="" Type="Normal" XCoord="1 2 3" YCoord="1 2 3">
				</Transition>
				<Transition Id="trans1.4" From="doctorWorkflow" To="nurseWorkflow" Name="Goto Nurse Workflow" Condition="" Type="Normal" XCoord="1 2 3" YCoord="1 2 3">
				</Transition>
				<Transition Id="trans1.5" From="doctorWorkflow" To="addPatientAgain" Name="Goto Add Patient Again" Condition="" Type="Normal" XCoord="1 2 3" YCoord="1 2 3">
				</Transition>
				<Transition Id="trans1.6" From="nurseWorkflow" To="addPatientAgain2" Name="Goto Add Patient Again 2" Condition="" Type="Normal" XCoord="1 2 3" YCoord="1 2 3">
				</Transition>
				<Transition Id="trans1.7" From="addPatientAgain" To="addPatientAgain2" Name="Goto Add Patient Again 2" Condition="" Type="Normal" XCoord="1 2 3" YCoord="1 2 3">
				</Transition>
				<Transition Id="trans1.8" From="addPatientAgain2" To="stopPatientWorkflow" Name="Stop Patient Workflow" Condition="" Type="Normal" XCoord="1 2 3" YCoord="1 2 3">
				</Transition>
            </Transitions>

        </WorkflowProcess>
    </WorkflowProcesses>
</Package>

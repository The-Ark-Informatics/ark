		<field iname="strUsrUsrgrpNam" sname="strUsrUsrgrpNam" ename="user_name" lname="User Name" cname="User Name" type="string" length="30" unique="false" required="true"/>
		<field iname="intUsergroupKey" sname="intUsergroupKey" ename="USERGROUPKEY" lname="Usergroup ID" cname="Usergroup ID" type="integer" length="8" unique="false" required="true"/>
		<field iname="intParentKey" sname="intParentKey" ename="USERGROUPKEY" lname="Usergroup ID" cname="Usergroup ID" type="integer" length="8" unique="false" required="true"/>
		<field iname="intDeleted" sname="intDeleted" ename="DELETED" lname="Deleted" cname="Deleted" type="integer" length="8" unique="false" required="false"/>
	</domain>
        
        <domain iname="GROUP" sname="GRP" ename="ix_group" sequence="ix_group_groupkey_seq" primarykey="GROUPKEY">
		<field iname="intGroupKey" sname="intGroupKey" ename="GROUPKEY" lname="Group ID" cname="Group ID" type="integer" length="8" unique="true" required="true"/>
		<field iname="strGroupName" sname="strGroupName" ename="NAME" lname="Group name" cname="Group name" type="string" length="100" unique="false" required="true"/>
		<field iname="strDescription" sname="strDescription" ename="DESCRIPTION" lname="Description" cname="Description" type="string" length="1000" unique="false" required="false"/>
	</domain>
        
        <domain iname="LOG" ename="ix_log" deletedfield="true" sequence="ix_log_seq" primarykey="LOGKEY">
               <field iname="intLogKey" ename="LOGKEY" lname="Log key" cname="Log key" type="integer" length="8" unique="true" required="true"/>
               <field iname="strType" ename="TYPE" lname="Log type" cname="Log type" type="string" length="30" unique="false" required="false"/>

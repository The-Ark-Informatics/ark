                <field iname="strLOVParentValue" sname="strLOVParentValue" ename="PARENTVALUE" lname="LOV Parent Value" cname="LOV Parent Value" type="string" length="1000" unique="false" required="false" />
                <field iname="intIsEditable" sname="intIsEditable" ename="ISEDITABLE" lname="Is Editable" cname="Is Editable" type="integer" length="1" unique="false" required="true" />
	</domain>

        <domain iname="ACTIVITY" sname="ACT" ename="ix_activity" deletedfield="true" sequence="ix_activity_activitykey_seq" primarykey="ACTIVITYKEY">
		<field iname="strActivityName" sname="strActivityName" ename="NAME" lname="Activity Name" cname="Activity Name" type="string" length="50" unique="true" required="true"/>
		<field iname="intActivityKey" sname="intActivityKey" ename="ACTIVITYKEY" lname="Activity ID" cname="Activity ID" type="integer" length="8" unique="false" required="true"/>
		<field iname="strActivityDesc" sname="strActivityDesc" ename="DESCRIPTION" lname="Activity Desc" cname="Activity Desc" type="string" length="100" unique="false" required="false"/>
		<field iname="intDeleted" sname="intDeleted" ename="DELETED" lname="Deleted" cname="Deleted" type="integer" length="8" unique="false" required="false"/>
	</domain>
        
        <domain iname="PERMISSION" sname="PERM" ename="ix_permission" sequence="ix_authorisation_seq" primarykey="PERMISSIONKEY">
		<field iname="intPermKey" sname="intPermKey" ename="PERMISSIONKEY" lname="Permission ID" cname="Permission ID" type="integer" length="8" unique="true" required="true"/>
		<field iname="intActivityKey" sname="intActivityKey" ename="ACTIVITYKEY" lname="Activity ID" cname="Activity ID" type="integer" length="8" unique="false" required="true"/>
		<field iname="intUsergroupKey" sname="intUsergroupKey" ename="USERGROUPKEY" lname="Usergroup ID" cname="Usergroup ID" type="integer" length="8" unique="false" required="true"/>
		<field iname="intGroupKey" sname="intGroupKey" ename="GROUPKEY" lname="Group ID" cname="Group ID" type="integer" length="8" unique="false" required="true"/>

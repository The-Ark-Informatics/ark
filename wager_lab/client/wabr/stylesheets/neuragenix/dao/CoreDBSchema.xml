<dbschema>

	<!--  <<<<<<<<<<<<<<<<<  DOMAIN DECLARATION  >>>>>>>>>>>>>>>>>>>>>>>> -->
        
        <domain iname="LOV" sname="LOV" ename="ix_listofvalues" deletedfield="true" sequence="ix_listofvalues_lovkey_seq" primarykey="LOVKEY">
		<field iname="intLOVID" sname="intLOVID" ename="LOVKEY" lname="LOV key" cname="LOV key" type="integer" length="8" unique="true" required="true"/>
		<field iname="strLOVType" sname="strLOVType" ename="TYPE" lname="LOV type" cname="LOV type" type="string" length="30" unique="false" required="false"/>
		<field iname="strLOVValue" sname="strLOVValue" ename="VALUE" lname="LOV value" cname="LOV value" type="string" length="30" unique="false" required="false"/>
		<field iname="intLOVSortOrder" sname="intLOVSortOrder" ename="SORTORDER" lname="Sort order" cname="Sort order" type="integer" length="30" unique="false" required="false"/>
		<field iname="intGroupKey" sname="intGroupKey" ename="GROUPKEY" lname="Group ID" cname="Group ID" type="integer" length="8" unique="false" required="false"/>
		<field iname="strDescription" sname="strDescription" ename="DESCRIPTION" lname="Description" cname="Description" type="string" length="100" unique="false" required="false"/>
		<field iname="intDeleted" sname="intDeleted" ename="DELETED" lname="Deleted" cname="Deleted" type="integer" length="8" unique="false" required="false"/>

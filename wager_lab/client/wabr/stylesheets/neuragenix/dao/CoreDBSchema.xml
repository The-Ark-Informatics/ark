               <field iname="strShortDesc" ename="SHORT_DESCRIPTION" lname="Short description" cname="Short description" type="string" length="50" unique="false" required="false"/>
               <field iname="strLongDesc" ename="LONG_DESCRIPTION" lname="Long description" cname="Long description" type="string" length="4000" unique="false" required="false"/>
               <field iname="strUser" ename="USER" lname="User" cname="User" type="string" length="50" unique="false" required="false"/>
               <field iname="intDeleted" ename="DELETED" lname="Deleted" cname="Deleted" type="integer" length="8" unique="false" required="false"/>
        </domain>

        
        
        
        
        
        
        
        
        
	<!--  <<<<<<<<<<<<<<<<<  END DOMAIN DECLARATION  >>>>>>>>>>>>>>>>>>>>>>>> -->        
        


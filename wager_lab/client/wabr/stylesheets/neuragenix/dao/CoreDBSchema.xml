	</domain>
        
        <domain iname="USERGROUP" sname="UGRP" ename="ix_usergroup" sequence="ix_profile_profilekey_seq" primarykey="USERGROUPKEY">
		<field iname="intUsergroupKey" sname="intUsergroupKey" ename="USERGROUPKEY" lname="Usergroup ID" cname="Usergroup ID" type="integer" length="8" unique="true" required="true"/>
		<field iname="strUsergroupName" sname="strUsergroupName" ename="NAME" lname="Usergroup name" cname="Usergroup name" type="string" length="100" unique="false" required="true"/>
		<field iname="strDescription" sname="strDescription" ename="DESCRIPTION" lname="Description" cname="Description" type="string" length="1000" unique="false" required="false"/>
		<field iname="intParentKey" sname="intParentKey" ename="PARENTUSERGROUP" lname="Parent ID" cname="Add to" type="integer" length="8" unique="false" required="true"/>
		<field iname="intDeleted" sname="intDeleted" ename="DELETED" lname="Deleted" cname="Deleted" type="integer" length="8" unique="false" required="true"/>
	</domain>
        

<?xml version='1.0' encoding='utf-8'?>


<dbschema type="postgres">

	<!--  Domains description  -->
	<domain iname="PROTEIN" ename="ix_protein" sequence="ix_protein_seq">
		<field iname="intProteinKey" ename="PROTEINKEY" lname="NA" cname="NA" type="integer" length="8" unique="true" required="false" />
		<field iname="strName" ename="NAME" lname="Protein name" cname="Protein name" type="string" length="50" unique="false" required="true" />
                <field iname="strGene" ename="GENE" lname="Gene" cname="Gene" type="string" length="50" unique="false" required="true" />
		<field iname="strLocalisation" ename="LOCALISATION" lname="Localisation" cname="Localisation" type="string" length="50" unique="false" required="true" />
		<field iname="intDeleted" ename="DELETED" lname="Deleted" cname="Deleted" type="integer" length="4" unique="false" required="false" />	                                
	</domain>
         
        <domain iname="ANTIBODY" ename="ix_antibody">
                <field iname="intAntibodyKey" ename="ANTIBODYKEY" lname="Antibody" cname="NA" type="integer" length="8" unique="true" required="false" />
		<field iname="intProteinKey" ename="PROTEINKEY" lname="Protein" cname="NA" type="integer" length="8" unique="true" required="true" />
		<field iname="strName" ename="NAME" lname="Antibody" cname="Antibody" type="string" length="50" unique="false" required="true" />
		
		<field iname="intDeleted" ename="DELETED" lname="Deleted" cname="Deleted" type="integer" length="4" unique="false" required="false" />	                                
	</domain>

        <formfield name="view_protein">
                <fieldname name="PROTEIN_intProteinKey" />
		<fieldname name="PROTEIN_strName" />
                <fieldname name="PROTEIN_strGene" />
		<fieldname name="PROTEIN_strLocalisation" />
                <fieldname name="PROTEIN_intDeleted" />
		
	</formfield>
        <formfield name="view_antibody">
                <fieldname name="ANTIBODY_intAntibodyKey" />
                <fieldname name="ANTIBODY_intProteinKey" />
		<fieldname name="ANTIBODY_strName" />
                <fieldname name="ANTIBODY_intDeleted" />
	</formfield>
        
</dbschema>